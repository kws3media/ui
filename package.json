{
  "name": "kws3-storybooks-frontend",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm-run-all dev:hmr",
    "dev:hmr": "concurrently \"storybook dev -p 9009 -s public\" \"node scripts/watcher/index.js\"",
    "build": "storybook build --docs -s public -o build",
    "populate": "grunt dist",
    "build:dist": "npm-run-all -s build -s populate",
    "deploy:dist": "grunt deploy:dist",
    "bootstrap": "lerna bootstrap --hoist",
    "publish:lerna": "lerna publish",
    "publish": "npm-run-all -s generate:dts-all -s publish:lerna",
    "list": "lerna list",
    "what_changed": "sh ./what_changed.sh",
    "lint": "eslint packages/ src/",
    "lint:fix": "eslint --fix packages/ src/",
    "sync:frameworkTypes": "node scripts/type_injector/syncer.js",
    "generate:dts-all": "node scripts/dts_generator/all.js",
    "generate:dts": "node scripts/dts_generator/index.js"
  },
  "dependencies": {
    "bulma": "^0.9.4",
    "global": "^4.4.0"
  },
  "devDependencies": {
    "@kws3/eslint-plugin-svelte3": "^1.0.1",
    "@mdx-js/react": "^2.3.0",
    "@storybook/addon-actions": "7.6.4",
    "@storybook/addon-backgrounds": "7.6.4",
    "@storybook/addon-controls": "7.6.4",
    "@storybook/addon-docs": "7.6.4",
    "@storybook/addon-essentials": "7.6.4",
    "@storybook/addon-interactions": "^7.6.4",
    "@storybook/addon-knobs": "7.0.2",
    "@storybook/addon-viewport": "7.6.4",
    "@storybook/addons": "7.6.4",
    "@storybook/blocks": "^7.6.4",
    "@storybook/builder-vite": "7.6.4",
    "@storybook/cli": "^7.6.4",
    "@storybook/jest": "0.2.2",
    "@storybook/mdx1-csf": "^1.0.0",
    "@storybook/mdx2-csf": "^1.1.0",
    "@storybook/source-loader": "7.6.4",
    "@storybook/svelte": "7.6.4",
    "@storybook/svelte-vite": "^7.6.4",
    "@storybook/testing-library": "0.2.1",
    "@storybook/theming": "7.6.4",
    "@storybook/addon-svelte-csf": "4.0.9",
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "comment-parser": "^1.2.3",
    "commenting": "^1.1.0",
    "concurrently": "^8.0.1",
    "ejs": "^3.1.9",
    "eslint": "^8.9.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-svelte": "^2.33.2",
    "glob": "^10.3.10",
    "grunt": "^1.6.1",
    "grunt-gitinfo": "^0.1.9",
    "grunt-sftp-deploy": "^0.2.5",
    "grunt-text-replace": "^0.4.0",
    "jest": "^29.7.0 ",
    "lerna": "^3.3.0",
    "moment": "^2.22.2",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "pluralize": "^8.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "sass": "^1.68.0",
    "storybook": "^7.6.4",
    "style-loader": "^3.3.3",
    "svelte": "^4.0.0",
    "svelte-eslint-parser": "^0.14.0",
    "svelte-preprocess": "^4.8.0",
    "svelte2tsx": "^0.6.11",
    "sveltedoc-parser": "https://github.com/ekhaled/sveltedoc-parser#custom",
    "ts-dedent": "^2.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.2.1"
  }
}
