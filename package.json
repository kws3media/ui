{
  "name": "kws3-storybooks-frontend",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all dev:hmr",
    "dev:hmr": "start-storybook -p 9009 -s public",
    "build": "build-storybook --docs -s public -o build",
    "populate": "grunt dist",
    "build:dist": "npm-run-all -s build -s populate",
    "deploy:dist": "grunt deploy:dist",
    "bootstrap": "lerna bootstrap --hoist",
    "publish": "lerna publish",
    "list": "lerna list",
    "what_changed": "sh ./what_changed.sh",
    "lint": "eslint packages/ src/",
    "lint:fix": "eslint --fix packages/ src/"
  },
  "dependencies": {
    "bulma": "^0.9.3",
    "global": "^4.3.2"
  },
  "devDependencies": {
    "@kws3/eslint-plugin-svelte3": "^1.0.1",
    "@ota-meshi/eslint-plugin-svelte": "^0.28.0",
    "@storybook/addon-actions": "6.5.10",
    "@storybook/addon-backgrounds": "6.5.10",
    "@storybook/addon-docs": "6.5.10",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-knobs": "6.3.1",
    "@storybook/addon-svelte-csf": "^2.0.7",
    "@storybook/addon-viewport": "6.5.10",
    "@storybook/addons": "6.5.10",
    "@storybook/jest": "0.0.10",
    "@storybook/source-loader": "6.5.10",
    "@storybook/svelte": "6.5.10",
    "@storybook/testing-library": "0.0.13",
    "chalk": "^4.0.0",
    "comment-parser": "^1.2.3",
    "commenting": "^1.1.0",
    "css-loader": "^0.28.7",
    "ejs": "^2.6.1",
    "eslint": "^8.9.0",
    "glob": "^7.1.7",
    "grunt": "^0.4.5",
    "grunt-gitinfo": "^0.1.9",
    "grunt-sftp-deploy": "^0.2.5",
    "grunt-text-replace": "^0.4.0",
    "jest": "^23.5.0",
    "lerna": "^3.3.0",
    "moment": "^2.22.2",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.3",
    "pluralize": "^8.0.0",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "sass": "^1.38.1",
    "sass-loader": "^10",
    "style-loader": "^0.19.1",
    "svelte": "^3.46.1",
    "svelte-eslint-parser": "^0.14.0",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.8.0",
    "sveltedoc-parser": "https://github.com/ekhaled/sveltedoc-parser#integration",
    "promis": "^1.1.4",
    "microevent": "https://github.com/ekhaled/microevent.js.git"
  }
}
