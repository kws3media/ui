{
  "name": "kws3-storybooks-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all dev:hmr",
    "dev:hmr": "start-storybook -p 9009 -s public",
    "build": "build-storybook -s public -o build",
    "populate": "grunt dist",
    "build:dist": "npm-run-all -s build -s populate",
    "deploy:dist": "grunt deploy:dist",
    "bootstrap": "lerna bootstrap --hoist",
    "publish": "lerna publish",
    "list": "lerna list"
  },
  "dependencies": {
    "bulma": "^0.9.3",
    "global": "^4.3.2",
    "sass": "^1.38.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.3.7",
    "@storybook/addon-backgrounds": "6.3.7",
    "@storybook/addon-essentials": "6.3.7",
    "@storybook/addon-knobs": "6.3.0",
    "@storybook/addon-svelte-csf": "1.1.0",
    "@storybook/addon-viewport": "6.3.7",
    "@storybook/addons": "6.3.7",
    "@storybook/source-loader": "6.3.7",
    "@storybook/addon-docs": "6.3.7",
    "@storybook/svelte": "6.3.7",
    "css-loader": "^0.28.7",
    "grunt": "^0.4.5",
    "grunt-gitinfo": "^0.1.9",
    "grunt-sftp-deploy": "^0.2.5",
    "grunt-text-replace": "^0.4.0",
    "jest": "^23.5.0",
    "lerna": "^3.3.0",
    "moment": "^2.22.2",
    "node-sass": "^6.0.0",
    "npm-run-all": "^4.1.3",
    "sass-loader": "^10",
    "style-loader": "^0.19.1",
    "svelte": "^3.42.1",
    "svelte-loader": "^3.1.2"
  }
}
