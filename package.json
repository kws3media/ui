{
  "name": "kws3-storybooks-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all dev:hmr",
    "dev:hmr": "start-storybook -p 9009 -s public",
    "build": "build-storybook -s public -o build",
    "populate":"grunt dist",
    "build:dist": "npm-run-all -s build -s populate",
    "deploy:dist": "grunt deploy:dist"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.m?js$": "babel-jest",
      "^.+\\.html$": "svelte-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "html"
    ]
  },
  "dependencies": {
    "global": "^4.3.2",
    "bulma": "0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@storybook/addon-actions": "4.0.0-alpha.20",
    "@storybook/addon-backgrounds": "4.0.0-alpha.20",
    "@storybook/addon-centered": "4.0.0-alpha.20",
    "@storybook/addon-knobs": "4.0.0-alpha.20",
    "@storybook/addon-links": "4.0.0-alpha.20",
    "@storybook/addon-notes": "4.0.0-alpha.20",
    "@storybook/addon-options": "4.0.0-alpha.20",
    "@storybook/addon-storyshots": "4.0.0-alpha.20",
    "@storybook/addon-storysource": "4.0.0-alpha.20",
    "@storybook/addon-viewport": "4.0.0-alpha.20",
    "@storybook/addons": "4.0.0-alpha.20",
    "@storybook/svelte": "4.0.0-alpha.20",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^0.28.7",
    "grunt": "^0.4.5",
    "grunt-gitinfo": "^0.1.8",
    "grunt-sftp-deploy": "^0.2.5",
    "grunt-text-replace": "^0.4.0",
    "jest": "^23.5.0",
    "moment": "^2.22.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "svelte": "^2.13.2",
    "svelte-jest": "^0.2.0",
    "svelte-loader": "^2.11.0"
  }
}
