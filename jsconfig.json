{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "checkJs": true,
    "strict": true,
    "noImplicitAny": false,
    "paths": {
      "@kws3/ui": ["./packages/@kws3/ui/index"],
      "@kws3/*": ["./packages/@kws3/*"]
    },
    "resolveJsonModule": true
  },
  "include": ["./packages/@kws3/ui/types/index.d.ts"],
  "exclude": ["node_modules"],
  "typeAcquisition": {
    "include": ["svelte"]
  }
}
