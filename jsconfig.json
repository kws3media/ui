{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "noImplicitAny": false,
    "paths": {
      "@kws3/ui": ["./packages/@kws3/ui/index"],
      "@kws3/*": ["./packages/@kws3/*"]
    },
    "resolveJsonModule": true,
    "typeRoots": ["./packages/@kws3/ui"],
    "types": ["svelte"],
    "declaration": true
  },
  "exclude": ["node_modules"]
}
